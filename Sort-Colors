//75 sorting colors..

class Solution {
public:
    void sortColors(vector<int>& nums) {
        vector<int> v(3);
        for(int i=0;i<nums.size();++i)
            v[nums[i]]++;
        nums.clear();
        for(int i=0;i<3;++i){
            for(int j=0;j<v[i];++j)
                nums.push_back(i);
        }
    }
    
};
